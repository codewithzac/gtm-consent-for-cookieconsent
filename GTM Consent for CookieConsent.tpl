// GTM template constants
const log = require('logToConsole');
const callInWindow = require('callInWindow');
const createQueue = require('createQueue');
const copyFromDataLayer = require('copyFromDataLayer');
const JSON = require('JSON');
const Object = require('Object');
const getType = require('getType');
const getTime = require('getTimestampMillis');
const isConsentGranted = require('isConsentGranted');
const gtagSet = require('gtagSet');
const updateConsentState = require('updateConsentState');
const setDefaultConsentState = require('setDefaultConsentState');
const getCookieValues = require('getCookieValues');
const localStorage = require('localStorage');

// Consent template constants
const consentConfig = callInWindow('CookieConsent.getConfig');
const dataLayerPush = createQueue('dataLayer');
const eventName = copyFromDataLayer('event');
log('Event triggering consent evaluation: ' + eventName);

// Event constants
const onFirstConsent = data.onFirstConsent;
const onChange = data.onChange;
const onUpdateEnabled = data.onUpdateEnabled;
const onUpdate = data.onUpdate;

// Function to validate user consent
function validateConsent(string) {
  const j = JSON.parse(string);

  if (!j) {
    log('Consent object cannot be parsed! We received: ' + string);
    return false;
  }

  if (j &&
      getType(j.categories) == 'array' &&
      getType(j.services) == 'object' &&
      getType(j.consentId) == 'string' &&
      getType(j.consentTimestamp) == 'string' &&
      getTime() < j.expirationTime) {
    log('Valid consent object found, contents: ' + string);
    return true;
  } else {
    log('Invalid consent object found, contents: ' + string);
    // CookieConsent attempts to apply consent preferences, even if the consent object is malformed.
    // Force a reset so consent is requested again on the next page, then fall back to default consent.
    callInWindow('CookieConsent.reset', true);
    return false;
  }
}

// Function to map consent JSON object to configured GTM consent types
function mapConsent(object) {
  let consentObj = {};

  // Find any category marked as strictly necessary
  let necessary = '';
  data.consentMapping.forEach(mapping => {
    if (mapping.gtmCategory == 'strictly_necessary') {
      necessary = mapping.ccCategory;
    }
  });

  // Categories from the consent object
  const defined = Object.keys(object.services).filter(item => item !== necessary);
  const accepted = object.categories.filter(item => item !== necessary);
  const rejected = defined.filter(item => accepted.indexOf(item) == -1);

  // Create mapping for accepted categories
  accepted.forEach(category => {
    data.consentMapping.forEach(mapping => {
      if (mapping.ccCategory == category) {
        consentObj[mapping.gtmCategory] = 'granted';
      }
    });
  });

  // Create mapping for rejected categories
  rejected.forEach(category => {
    data.consentMapping.forEach(mapping => {
      if (mapping.ccCategory == category) {
        consentObj[mapping.gtmCategory] = 'denied';
      }
    });
  });

  // Determine the number of differences between current consent state and what we're about to set
  // Don't attempt to read current consent state at consent initialisation - this causes errors!
  let consentChanges = 0;
  if (eventName != 'gtm.init_consent') {
    for (let i=0; i<Object.keys(consentObj).length; i++) {
      // Consent granted
      if (Object.values(consentObj)[i] == 'granted') {
        if (!isConsentGranted(Object.keys(consentObj)[i])) {
          consentChanges++;
        }
      }
      // Consent denied
      if (Object.values(consentObj)[i] == 'denied') {
        if (isConsentGranted(Object.keys(consentObj)[i])) {
          consentChanges++;
        }
      }
    }
  }

  log('Setting updated consent using data from the consent object: ' + JSON.stringify(consentObj));
  updateConsentState(consentObj);

  // If enabled - trigger consent update dataLayer event for:
  // - the "onFirstConsent" CookieConsent event, or
  // - the "onChange" CookieConsent event (if there are changes)
  if (onUpdateEnabled) {
    if ((eventName == onFirstConsent) || (eventName == onChange && consentChanges > 0)) {
      log(eventName == onFirstConsent ?
        'First consent - triggering ' + onUpdate + ' event' :
        'Consent changed (' + consentChanges + ' changes)  - triggering ' + onUpdate + ' event');
      consentObj.event = onUpdate;
      consentObj.consent_id = object.consentId;
      consentObj.consent_timestamp = object.consentTimestamp;    
      dataLayerPush(consentObj);
    }
  }
  return true;
}

// Function to map default consent to configured GTM consent types
// Prefer mode from CookieConsent config if available
function mapDefault() {
  const source = consentConfig ? 'CookieConsent' : 'tag';
  const mode = consentConfig ? consentConfig.mode : data.mode;
  const consentObj = data.consentMapping.reduce((obj, mapping) => {
    if (mapping.gtmCategory != 'strictly_necessary') {
      obj[mapping.gtmCategory] = mode == 'opt-out' ? 'granted' : 'denied';
    }
    return obj;
  }, {});
  
  if (data.wait_for_update > 0) {
    consentObj.wait_for_update = data.wait_for_update;
  }

  log('Setting default consent using ' + source + ' configuration, mode is ' + mode + ': ' + JSON.stringify(consentObj));
  setDefaultConsentState(consentObj);
  return true;
}

// Set advanced settings
gtagSet({
  url_passthrough: data.url_passthrough || false,
  ads_data_redaction: data.ads_data_redaction || false
});

// Configure consent:
// 1. Try to read / validate / map consent string first, then use that to update consent
// 2. If any of the above fails, fall back to default consent
const consentString = data.use_local_storage ? localStorage.getItem('cc_cookie') : getCookieValues(data.cookie_name)[0];

const hasValidConsent = 
  getType(consentString) == 'string' && 
  validateConsent(consentString) &&
  mapConsent(JSON.parse(consentString));

if (!hasValidConsent) {
  mapDefault();
}

// Call data.gtmOnSuccess when the tag is finished.
data.gtmOnSuccess();


